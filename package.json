{
  "name": "kth-bch-native",
  "version": "0.4.0",
  "description": "Bitcoin, Bitcoin Cash and Litecoin development platform for Javascript applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k-nuth/js-native.git"
  },
  "keywords": [
    "bitcoin",
    "cash",
    "litecoin",
    "money",
    "kth"
  ],
  "author": "Fernando Pelliccioni <fpelliccioni@gmail.com> (http://kth.cash)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k-nuth/js-native/issues"
  },
  "homepage": "https://github.com/k-nuth/js-native#readme",
  "engines": {
    "node": ">=4.2.4"
  },
  "main": "index.js",
  "binary": {
    "module_name": "kth-bch-native",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/k-nuth/js-native/releases/download/",
    "remote_path": "{version}"
  },
  "dependencies": {
    "nan": "^2.7.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node index.js",
    "prepublish": "npm ls"
  }
}

